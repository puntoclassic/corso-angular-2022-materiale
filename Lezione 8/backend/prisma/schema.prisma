// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model product {
  id Int @id @default(autoincrement()) 
  marca String
  codice String
  prezzo Decimal
  disponibile Boolean @default(false)
  ean String
  nome String
  descrizione String
}
